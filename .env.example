# Copy to .env and fill in your Supabase project values.
# Get these from Supabase Dashboard → Project Settings → API

EXPO_PUBLIC_SUPABASE_URL=
EXPO_PUBLIC_SUPABASE_ANON_KEY=

# Set to "true" to use outfit images from mock data (data/mock-inspiration.json) instead of the database.
# Use this if your DB has old image URLs or you want to demo without seeding.
EXPO_PUBLIC_USE_MOCK_FEED=true
